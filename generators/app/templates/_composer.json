{
  "name": "<%=VENDOR_SLUG%>/<%=APP_SLUG%>",
  "description": "",
  "keywords": ["PHP", "CLI"],
  "authors": [
    {
      "name": "<%=AUTHOR_NAME%>",
      "email": "<%=AUTHOR_EMAIL%>"
    }
  ],
  "homepage": "https://github.com/<%=VENDOR_SLUG%>/<%=APP_SLUG%>",
  "require": {
    "php": "^7.1.3",
    "symfony/console": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "orchestra/testbench": "3.8.*",
    "phpstan/phpstan": "^0.11.8"
  },
  "license": "<%=APP_LICENSE%>",
  "autoload": {
    "psr-4": {
      "<%=VENDOR_NAMESPACE_COMPOSER%>\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "<%=VENDOR_NAMESPACE_COMPOSER%>\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
    "analysis": "vendor/bin/phpstan analyse"
  }
}
